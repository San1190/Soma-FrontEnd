{
  "name": "soma",
  "version": "1.0.0",
  "license": "0BSD",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@expo-google-fonts/afacad": "^0.4.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/cli-server-api": "^20.0.2",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.4.10",
    "axios": "^1.12.2",
    "expo": "~54.0.29",
    "expo-av": "~16.0.8",
    "expo-font": "^14.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "^0.32.15",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "memoize-one": "^6.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "recharts": "^3.3.0",
    "sweetalert2": "^11.26.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.1",
    "react-test-renderer": "19.1.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-gifted-charts|react-native-paper|react-native-vector-icons)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/__tests__/**/*.test.jsx"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.styles.js",
      "!**/node_modules/**"
    ]
  },
  "proxy": "http://localhost:8080"
}
